{
  "displayName": "AI Broker Operational Dashboard",
  "mosaicLayout": {
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Error Rate - HTTP 5xx Responses",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/veria/ai-broker/error_rate\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_MEAN",
                      "groupByFields": ["resource.label.service_name", "resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "Error Rate - ${resource.label.service_name}/${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Error Rate (%)",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            },
            "thresholds": [
              {
                "value": 2.0,
                "color": "RED",
                "direction": "ABOVE",
                "label": "SLA Threshold (2%)"
              },
              {
                "value": 1.0,
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "Warning Threshold (1%)"
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "xPos": 6,
        "widget": {
          "title": "P95 Latency Distribution",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/veria/ai-broker/p95_latency\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95",
                      "crossSeriesReducer": "REDUCE_PERCENTILE_95",
                      "groupByFields": ["resource.label.service_name", "resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "STACKED_BAR",
                "targetAxis": "Y1",
                "legendTemplate": "P95 Latency - ${resource.label.service_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Response Time (ms)",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            },
            "thresholds": [
              {
                "value": 2000.0,
                "color": "RED",
                "direction": "ABOVE",
                "label": "SLA Threshold (2000ms)"
              },
              {
                "value": 1500.0,
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "Warning Threshold (1500ms)"
              }
            ]
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "yPos": 4,
        "widget": {
          "title": "CPU Utilization",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/cpu/utilizations\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_MEAN",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "CPU - ${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "CPU Utilization",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            },
            "thresholds": [
              {
                "value": 0.8,
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "High CPU (80%)"
              }
            ]
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "xPos": 4,
        "yPos": 4,
        "widget": {
          "title": "Memory Utilization",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/memory/utilizations\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_MEAN",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "Memory - ${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Memory Utilization",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            },
            "thresholds": [
              {
                "value": 0.9,
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "High Memory (90%)"
              }
            ]
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "xPos": 8,
        "yPos": 4,
        "widget": {
          "title": "Network I/O",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/network/received_bytes_count\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["resource.label.service_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "Bytes Received/sec"
              },
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/network/sent_bytes_count\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["resource.label.service_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "Bytes Sent/sec"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Bytes/sec",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 3,
        "yPos": 7,
        "widget": {
          "title": "Active Alerts Status",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"monitoring.googleapis.com/uptime_check/check_passed\" AND resource.type=\"uptime_url\"",
                "aggregation": {
                  "alignmentPeriod": "300s",
                  "perSeriesAligner": "ALIGN_FRACTION_TRUE",
                  "crossSeriesReducer": "REDUCE_MEAN"
                }
              }
            },
            "sparkChartView": {
              "sparkChartType": "SPARK_BAR"
            },
            "gaugeView": {
              "lowerBound": 0.0,
              "upperBound": 1.0
            },
            "thresholds": [
              {
                "value": 0.95,
                "color": "GREEN",
                "direction": "ABOVE",
                "label": "Healthy"
              },
              {
                "value": 0.90,
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "Warning"
              },
              {
                "value": 0.90,
                "color": "RED",
                "direction": "BELOW",
                "label": "Critical"
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 3,
        "xPos": 6,
        "yPos": 7,
        "widget": {
          "title": "Request Volume & Success Rate",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_count\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["metric.label.response_code_class"]
                    }
                  }
                },
                "plotType": "STACKED_BAR",
                "targetAxis": "Y1",
                "legendTemplate": "${metric.label.response_code_class}xx Responses"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Requests/sec",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "yPos": 10,
        "widget": {
          "title": "Service Health Matrix",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/instance_count\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "STACKED_BAR",
                "targetAxis": "Y1",
                "legendTemplate": "Instances - ${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Active Instances",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            },
            "thresholds": [
              {
                "value": 0.0,
                "color": "RED",
                "direction": "BELOW",
                "label": "No Active Instances"
              }
            ]
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "xPos": 4,
        "yPos": 10,
        "widget": {
          "title": "Deployment Correlation",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/container/billable_instance_time\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "STACKED_AREA",
                "targetAxis": "Y1",
                "legendTemplate": "Billable Time - ${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Billable Seconds/sec",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        }
      },
      {
        "width": 4,
        "height": 3,
        "xPos": 8,
        "yPos": 10,
        "widget": {
          "title": "Traffic Distribution",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_DELTA",
                      "crossSeriesReducer": "REDUCE_PERCENTILE_50",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "P50 Latency - ${resource.label.revision_name}"
              },
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" AND resource.type=\"cloud_run_revision\" AND resource.label.service_name=\"ai-broker\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_DELTA",
                      "crossSeriesReducer": "REDUCE_PERCENTILE_99",
                      "groupByFields": ["resource.label.revision_name"]
                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y1",
                "legendTemplate": "P99 Latency - ${resource.label.revision_name}"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "Latency (ms)",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        }
      }
    ]
  },
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "service_name",
      "templateVariable": "service_name",
      "stringValue": "ai-broker"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "revision_name",
      "templateVariable": "revision_name"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "environment",
      "stringValue": "us-central1"
    }
  ],
  "etag": "generated-dashboard-v1"
}